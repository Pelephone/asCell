	import netSocket.MessageData;
	
	/**	 * 发送协议基类	 * @author Michael.Huang	 *	 */	public class SendMessage extends AbstractMessage	{		/**		 * SendMessage construct 		 * @param msgId 协议ID		 * 		 */				public function SendMessage(msgId:int)		{			this.msgId = msgId;			super.msgData = new MessageData(msgId);		}		/**		 * @inheritDoc		 */		override public function get msgData():MessageData		{			if (null == super.msgData)			{				super.msgData = new MessageData(msgId);			}			return super.msgData;		}		/**		 * @inheritDoc		 */		public function toString():String		{			var res:String = "";			res = "[class ReceiveMessage] : 消息解析 >> " + res;			return res;		}	}}